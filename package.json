    "cookie-universal-nuxt": "^2.1.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "nuxt": "^2.13.3",
    "nuxt-i18n": "^6.10.1",
